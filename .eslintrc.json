{
	"extends": "next/core-web-vitals",
	"rules": {
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"camelcase": "error",
		// "curly": "error",
		"eqeqeq": "error",
		"no-var": "error",
		"prefer-const": "error",
		"semi": [
			"error",
			"never"
		],
		"brace-style": [
			"error",
			"allman"
		],
		"keyword-spacing": "error",
		"array-bracket-spacing": "error",
		"object-curly-spacing": "error"
	}
}